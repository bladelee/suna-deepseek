# Environment Configuration Example
# Copy this file to .env and fill in your actual values

# Environment mode
ENV_MODE=local

# LLM API Keys (at least one is required)
# OpenAI API Key - 如果你要使用OpenAI模型
OPENAI_API_KEY=your_openai_api_key_here

# DeepSeek API Key - 如果你要使用DeepSeek模型（推荐，支持中文）
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Anthropic API Key - 如果你要使用Claude模型
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Groq API Key - 如果你要使用Groq模型
# GROQ_API_KEY=your_groq_api_key_here

# OpenRouter API Key - 如果你要使用OpenRouter模型
# OPENROUTER_API_KEY=your_openrouter_api_key_here

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_SSL=false

# Other API Keys
TAVILY_API_KEY=your_tavily_api_key_here
RAPID_API_KEY=your_rapid_api_key_here
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Local Docker Sandbox Configuration
USE_LOCAL_DOCKER_SANDBOX=true
SANDBOX_IMAGE_NAME=kortix/suna:0.1.3.6
SANDBOX_SNAPSHOT_NAME=kortix/suna:0.1.3.6
SANDBOX_ENTRYPOINT=/usr/bin/supervisord -n -c /etc/supervisor/conf.d/supervisord.conf

# Optional: Daytona Configuration (will be ignored when USE_LOCAL_DOCKER_SANDBOX=true)
# DAYTONA_API_KEY=your_daytona_api_key_here
# DAYTONA_SERVER_URL=your_daytona_server_url_here
# DAYTONA_TARGET=your_daytona_target_here
