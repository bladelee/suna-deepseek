  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/colorthief": "^2.6.0",
    "@types/diff": "^7.0.2",
    "@types/jju": "^1.4.5",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "electron": "^31.0.0",
    "electron-builder": "^24.0.0",
    "encoding": "^0.1.13",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "prettier": "^3.5.3",
